{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "name": "ice_example",
  "scripts": {
    "setup": "npm i && dfx canister create ice_test_backend && dfx generate ice_test_backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "ice:create": "ice run ice_test_backend:create",
    "ice:build": "ice run ice_test_backend:build",
    "ice:install": "ice run ice_test_backend:install",
    "ice:ii:create": "ice run internet_identity:create",
    "ice:ii:build": "ice run internet_identity:build",
    "ice:ii:bindings": "ice run internet_identity:bindings",
    "ice:ii:install": "ice run internet_identity:install",
    "ice:c": "ice c",
    "ice:ui": "ice ui",
    "ice:icrc1:create": "ice run icrc1_ledger:create",
    "ice:icrc1:build": "ice run icrc1_ledger:build",
    "ice:icrc1:bindings": "ice run icrc1_ledger:bindings",
    "ice:icrc1:install": "ice run icrc1_ledger:install",
    "canisters:create": "ice canisters:create",
    "canisters:build": "ice canisters:build",
    "canisters:bindings": "ice canisters:bindings",
    "canisters:install": "ice canisters:install",
    "canisters": "ice canisters",
    "list": "ice list",
    "status": "ice canisters:status",
    "help": "ice --help"
  },
  "type": "module",
  "workspaces": [
    "src/ice_test_frontend"
  ],
  "dependencies": {
    "@dfinity/ckbtc": "^3.1.7",
    "@dfinity/cketh": "^3.4.4",
    "@dfinity/cmc": "^5.0.0",
    "@dfinity/ic-management": "^6.0.4",
    "@dfinity/ledger-icp": "^2.6.8",
    "@dfinity/ledger-icrc": "^2.7.3",
    "@dfinity/nns": "^8.2.2",
    "@dfinity/sns": "^3.2.9",
    "@ice.ts/canisters": "file:../ice/packages/canisters",
    "@ice.ts/runner": "file:../ice/packages/runner",
    "@nfid/identitykit": "^1.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  }
}
